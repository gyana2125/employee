<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="emp.employeelookup.controller.home">
	<App>
		<pages>
			<Page title="home">
				<content>
					
					
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Employee" press="emplist">
						<TileContent unit="List">
							<NumericContent scale="%" value="{}" valueColor="Critical" indicator="Down"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Employee" press="empOverview">
						<TileContent unit="Table">
							<NumericContent scale="%" value="{}" valueColor="Critical" indicator="Down"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Display not Found" press="onDisplayNotFound">
						<TileContent unit="List">
							<content>
							
							</content>
						</TileContent>
					</GenericTile>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>